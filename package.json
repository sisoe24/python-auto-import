{
	"name": "python-auto-import",
	"displayName": "Python Auto Import",
	"description": "Suggest Auto-import for third-party modules based on modules already imported.",
	"version": "0.1.0",
	"publisher": "virgilsisoe",
	"repository": {
		"type": "git",
		"url": "https://github.com/sisoe24/python-auto-import"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:python"
	],
	"keywords": [
		"python",
		"auto-import",
		"import",
		"auto",
		"third-party"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "python-auto-import.autoImport",
				"title": "Python Auto Import"
			}
		],
		"configuration": [
			{
				"title": "Python Auto Import",
				"properties": {
					"pythonAutoImport.pythonInterpreterPath": {
						"type": "string",
						"description": "A Python interpreter path to inspect for module auto-suggestions."
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.64.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}
